    "pre-commit": "^1.2.2",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.8.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "ts-jest": "^26.4.1",
    "typescript": "^4.1.3",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.11"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "vue-cli-service lint",
    "*.{vue,htm,html,css,sss,less,scss,sass}": "stylelint"
  },
  "pre-commit": "lint-staged",
  "jest": {
    "moduleFileExtensions": [
      "vue",
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.ts$": "ts-jest",
      ".*\\.vue$": "vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/components/shared/**/*.{ts,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": -10
      }
    }
  }
}
