    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.5.1",
    "vue-router": "^3.1.6",
    "vue-the-mask": "^0.11.1",
    "vuelidate": "^0.7.6",
    "vuelidate-property-decorators": "^1.0.28",
    "vuetify": "^2.2.11",
    "vuex": "^3.1.3",
    "vuex-simple": "^2.2.0",
    "vuex-smart-module": "^0.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/preset-env": "^7.12.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.165",
