    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.3.0",
    "@mdi/font": "^5.7.55",
    "axios": "^0.19.2",
    "babel-core": "^7.0.0-bridge.0",
    "core-js": "^3.19.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "utils": "^0.3.1",
